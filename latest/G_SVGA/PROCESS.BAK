}
int pull_over(Pp *state,Car *p,int n,int a)   //停车过程函数
{
    int gd=VGA;
    int gm=VGAMED;
	int i,k=1,s,x,y,p=0;
	int *f=&p;
	sp ob[21];
	 for(i=0;i<n;i++)
	{                          //初始化小车模块
	   init_sp(&ob[i],X0,Y);
	}
	initgraph(&gd,&gm,"bgi");
	for(s=0;;s++)
	{

		setactivepage(1);
		cleardevice();

		extra_car(state,p,ob,a,k,f);


		setvisualpage(1);

	   delay(14);
	   setactivepage(0);
	   cleardevice();

	   extra_car(state,p,ob,a,k,f);


	   setvisualpage(0);
		if(k<n)
		{ k=1+s/70;}

	   delay(14);
	   if(kbhit()) break;
	   if(f==(n)) break;

	}
	Delay(30);
	getch();
	closegraph();
	return 13;    //选择页
   }

