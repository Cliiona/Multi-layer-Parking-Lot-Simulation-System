void Init_outsp(sp*p,Car *cr)       //小车模块初始化
{
    switch(cr->j)
    {
	   case 0: p->frame=2;
			   p->x=502;
			   p->pro=0;
			   break;
	   case 1: p->frame=6;
			   p->x=265;
			   p->pro=0;
			   break;
	   case 2: p->frame=6;
			   p->x=135;
			   p->pro=2;
			   break;

	}
	 p->y=283-N*(cr->k);
	 p->state=0;
	 p->fx=0;
}
void car_message(Pp *state,Car *cr,int f)        //f楼层          全层取车，信息设置
{
	int j,k;
	int i=0;
	for(j=0;j<3;j++)
	{
        switch(j)
        {
            case 0:
            case 2:
                for(k=0;k<7;k++)
		{
           if (state->a[f][j][k])
           {
               init_car(&cr[i]);
               cr[i].i=f;
               cr[i].j=j;
               cr[i].k=k;
               //state->a[f][j][k]=5;       //状态即将被取车
               road_design(&cr[i]);
               i++;
              }
        }
                       break;
            case 1:
            	for(k=6;k>=0;k--)
		{
           if (state->a[f][j][k])
           {
               init_car(&cr[i]);
               cr[i].i=f;
               cr[i].j=j;
               cr[i].k=k;
               //state->a[f][j][k]=5;       //状态即将被取车
               road_design(&cr[i]);
               i++;
              }
        }
                break;
           
        
        }
    }
    
} 
